<%- include("partials/header.ejs"); %>

<h1>Home</h1>
<p> <%= startingHomeContent %> </p>

<% posts.forEach(function(post) { %>
<h1> <%= post.newTitle %> </h1>
<p>
  <%= post.newPost.substring(0, 160) + " ..." %>
  <a href="/posts/<%= post.newTitle %>">Read More</a>
</p>
<% }); %>


<%- include("partials/footer.ejs"); %>
